		super.onCreate(savedInstanceState);
		
		setContentView(R.layout.danxuan);
		
		
		
		/* 以findViewById() 取得对象*/
		mySpinner = (Spinner) findViewById(R.id.spinner_City);
	
		
		
	}
	
}
